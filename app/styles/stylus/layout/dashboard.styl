.dashboard
    min-width 600px
    margin-left 100px
    margin-right 100px
    position relative

    box-shadow 0px 10px 30px 0px rgba(0, 0, 0, 0.06)

    transform-origin top left
    overflow hidden

.dashboard__content
    width 100%
    height 500px
    padding-left 20px
    padding-right 20px
    padding-top 20px
    background-color $white